{
  "name": "Irur",
  "version": "0.5.4",
  "slug": "irur",
  "description": "Infrared Universal Remote webUI for Tasmota",
  "url": "https://github.com/johanson/irur",
  "arch": [
    "amd64",
    "armv7",
    "armhf",
    "i386",
    "aarch64"
  ],
  "armor": true,
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8099]",
  "ingress": true,
  "panel_title": "IR Remote",
  "panel_icon": "mdi:remote",
  "boot": "auto",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "options": {
    "topic_listen": "irur/tele/RESULT",
    "topic_send": [
      "irur/cmnd/IRsend"
    ],
    "dark_theme": false
  },
  "schema": {
    "topic_listen": "str",
    "topic_send": [
      "str"
    ]
  },
  "ports": {
    "8099/tcp": null
  },
  "ports_description": {
    "8099/tcp": "Web interface (Not required for Ingress)"
  }
}